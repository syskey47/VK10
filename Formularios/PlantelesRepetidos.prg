SELECT 0
USE NTA_PLANTELES

DO WHILE .T.

	SELECT IdMatricula, ;
			IdGrado, ;
			COUNT(*) AS TOTAL ;
		FROM NTA_PLANTELES ;
		GROUP BY 1, 2 ;
		HAVING TOTAL > 1 ;
		INTO CURSOR curTEMPORAL

	IF	_TALLY > 0

		SCAN

			SELECT NTA_PLANTELES
			LOCATE FOR NTA_PLANTELES.IdMatricula = curTEMPORAL.IdMatricula AND ;
				NTA_PLANTELES.IdGrado = curTEMPORAL.IdGrado

			IF	FOUND()
				DELETE
			ENDIF

		ENDSCAN
		
	ELSE
	
		EXIT

	ENDIF
	
ENDDO

CLOSE TABLES
